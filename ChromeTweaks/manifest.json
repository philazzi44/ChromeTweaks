{
   "browser_action": {
      "default_icon": "icon64.png",
      "default_title": "Phil - Chrome Tweaks"
   },
   "content_scripts": [ {
      "css": [  ],
      "js": [ "jquery-2.0.2.min.js", "jquery-ui-1.10.3.min.js", "content.js" ],
      "matches": [ "http://*/*", "https://*/*" ],
      "run_at": "document_idle"
   } ],
   "description": "Tweaks things as I see fit.",
   "icons": {
      "16": "icon16.png",
      "32": "icon32.png",
      "48": "icon48.png",
      "64": "icon64.png"      
   },
   "incognito": "split",
   "manifest_version": 2,
   "name": "Phil Tweak",
   "options_page": "options.html",
   "permissions": [ "tabs", "clipboardWrite", "storage" ],
   "background": { "page": "background.html" },
   "version": "1.0",
   "web_accessible_resources": [ "changeicon.png", "bugicon.gif", "copyicon.png", "copydoneicon.png", "copyurldoneicon.png" ]
}